{
    "importOrder": [
        "^react$",
        "^@/types/(.*)$",
        "./types",
        "<THIRD_PARTY_MODULES>",
        "^@/config/(.*)$",
        "^@/hooks/(.*)$",
        "^@/service/(.*)$",
        "^@/components/(.*)$",
